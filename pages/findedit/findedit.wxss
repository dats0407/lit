}

.addImg image{
  width: 100%;
  height: 100%;

}

.uploaded{
  height: 750rpx;
  width: 100%;
  float: left;
  margin-top: 20rpx;
}

.uploaded .addbtn{
  width: 120rpx;
  height: 120rpx;
}

.uploaded image{
  width: 210rpx;
  height: 210rpx;
  margin-top: 20rpx;
  float: left;
  margin-left: 15rpx;
}

.uploaded .delImg{
  width: 50rpx;
  height: 50rpx;
