}

.title{
  width: 100%;
  height: 100rpx;
  background-color: #FFF;
}

.title .cancelnav{
  width: 20%;
  height: 100%;
  float: left;
  text-align: left;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.title .cancelnav text{
  font-size: 16px;
  margin-bottom: 10rpx;
  text-align: left;
}

.title .submitview{
  width: 20%;
  height: 100%;
  float: right;
  text-align: right;
  margin-right: 20rpx;
  margin-top: 20rpx;
}

.title .submitview text{
  font-size: 16px;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  text-align: right;
  color: #009a61;
}


.textcont{
  width: 100%;
  height: 500rpx;
}

.textcont textarea{
